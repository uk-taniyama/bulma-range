{
  "name": "bulma-range",
  "version": "1.0.2",
  "description": "Display classic slider more sexy, in different colors, sizes, and states ",
  "main": "dist/js/bulma-slider.min.js",
  "style": "dist/css/bulma-slider.min.css",
  "sass": "src/sass/index.sass",
  "scripts": {
    "build": "gulp",
    "commit": "git-cz",
    "precommit": "npm test",
    "travis-deploy-once": "travis-deploy-once",
    "vup:patch": "standard-version --release-as patch",
    "vup:minor": "standard-version --release-as minor",
    "vup:major": "standard-version --release-as major",
    "watch:test": "jest --testEnvironment jest-environment-jsdom test/index.test.js -w",
    "test": "jest --testEnvironment jest-environment-jsdom test/index.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uk-taniyama/bulma-range.git"
  },
  "keywords": [
    "Bulma",
    "bulma",
    "bulma.io",
    "extensions",
    "range"
  ],
  "author": "uk-taniyama <uk.taniyama@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uk-taniyama/bulma-range/issues"
  },
  "homepage": "https://github.com/uk-taniyama/bulma-range#readme",
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.19.4",
    "@babel/preset-env": "^7.19.4",
    "ansi-colors": "^4.1.3",
    "autoprefixer": "^10.4.12",
    "babel-jest": "^29.2.1",
    "babel-loader": "^8.2.5",
    "babel-preset-env": "^1.7.0",
    "bulma": "^0.9.4",
    "camelcase": "6.3.0",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "del": "6.1.1",
    "fancy-log": "^2.0.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-nop": "^0.0.3",
    "gulp-postcss": "^9.0.1",
    "gulp-sass": "^5.1.0",
    "gulp-uglify": "^3.0.2",
    "jest": "^29.2.1",
    "jest-environment-jsdom": "^29.2.1",
    "regenerator-runtime": "^0.13.10",
    "sass": "^1.55.0",
    "standard-version": "^9.5.0",
    "travis-deploy-once": "^5.0.11",
    "webpack": "^5.74.0",
    "webpack-stream": "^7.0.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "files": [
    "dist/**",
    "src/**",
    "_bulma-range.sass",
    "LICENSE",
    "README.md"
  ]
}
